<script>
    $(document).ready(function () {
        var empProjectDataList = @Html.Raw(Json.Encode(data));

        if (empProjectDataList != null && empProjectDataList.length > 0) {


            $("#monthlySalesChart").empty();

            var categories = [];
            var data = [];
            debugger

            $.each(empProjectDataList, function (index, item) {
                categories.push(item.ProjectTitle);
                data.push(item.overallPercentage);
            });

            var options = {
                chart: {
                    type: 'bar',
                    height: '318',
                    parentHeightOffset: 0,
                    foreColor: '#6c757d',
                    background: '#ffffff',
                    toolbar: {
                        show: false
                    },
                },
                theme: {
                    mode: 'light'
                },
                tooltip: {
                    theme: 'light'
                },
                colors: ['#1aa7a6'],
                fill: {
                    opacity: .9
                },
                grid: {
                    padding: {
                        bottom: -4
                    },
                    borderColor: '#D6E9E9',
                    xaxis: {
                        lines: {
                            show: true
                        }
                    }
                },
                series: [{
                    name: 'Completed',
                    data: data // Dynamic data
                }],
                xaxis: {
                    type: 'category',
                    categories: categories, // Dynamic categories
                    axisBorder: {
                        color: '#dee2e6',
                    },
                    axisTicks: {
                        color: '#dee2e6',
                    },
                },
                yaxis: {
                    min: 0,
                    max: 100,
                    tickAmount: 5,
                    title: {
                        text: 'Project Completion',
                        style: {
                            fontSize: '12px',
                            color: '#adb5bd',
                            fontFamily: 'inherit',
                        }
                    },
                },
                legend: {
                    show: true,
                    position: "top",
                    horizontalAlign: 'center',
                    fontFamily: 'inherit',
                    itemMargin: {
                        horizontal: 8,
                        vertical: 0
                    },
                },
                stroke: {
                    width: 0
                },
                dataLabels: {
                    enabled: true,
                    style: {
                        fontSize: '10px',
                        fontFamily: 'inherit',
                    },
                    offsetY: -27
                },
                plotOptions: {
                    bar: {
                        columnWidth: "50%",
                        borderRadius: 4,
                        dataLabels: {
                            position: 'top',
                            orientation: 'vertical',
                        }
                    },
                },
            };
        }
        // Render the chart
        var apexBarChart = new ApexCharts(document.querySelector("#monthlySalesChart"), options);
        apexBarChart.render();
        if ($('#storageChart').length) {
            var colors = {
                primary: '#029e9d', // Set the primary color to #029e9d
                light: '#f3f3f3',   // You can define other colors like light, muted, etc.
                muted: '#6c757d',
                bodyColor: '#212529'
            };
            var options = {
                chart: {
                    height: 260,
                    type: "radialBar"
                },
                series: [@Model.TotaCompleteProject],
                colors: [colors.primary],
                plotOptions: {
                    radialBar: {
                        hollow: {
                            margin: 15,
                            size: "70%"
                        },
                        track: {
                            show: true,
                            background: colors.light,
                            strokeWidth: '100%',
                            opacity: 1,
                            margin: 5,
                        },
                        dataLabels: {
                            showOn: "always",
                            name: {
                                offsetY: -11,
                                show: true,
                                color: colors.muted,
                                fontSize: "13px"
                            },
                            value: {
                                color: colors.bodyColor,
                                fontSize: "30px",
                                show: true
                            }
                        }
                    }
                },
                fill: {
                    opacity: 1
                },
                stroke: {
                    lineCap: "round",
                },
                labels: ["Project Data"]
            };

            var chart = new ApexCharts(document.querySelector("#storageChart"), options);
            chart.render();
        }

    });
</script>


