<script>
    $(document).ready(function () {
        $(".filter1").change()
    })

  $(document).ready(function () {
    // Pass the Razor data as JSON to the JavaScript context
    var yearlyData = [
       @(Convert.ToInt32(Model.SelfCreatedProject) + Convert.ToInt32(Model.TotalAssignProject)),
       @Convert.ToInt32(Model.SelfCreatedProject),
       @Convert.ToInt32(Model.TotalAssignProject)
    ];

    @*var monthlyData = [@Convert.ToInt32(@data.monTotalProject),@Convert.ToInt32(data.monInternalProject), @Convert.ToInt32(data.monExternalProject)];  // Example monthly data*@

      // Set the initial data for the chart (monthly by default)
      var datac = yearlyData;

    // Get the chart context
    var ctx = document.getElementById('pichart').getContext('2d');

    // Initialize chart with default data (monthly)
    var pichart = new Chart(ctx, {
        type: 'pie',
        data: {
            labels: ['Total', 'Self Created', 'Assigned'],
            datasets: [{
                label: 'Project Overview',
                data: datac,
                backgroundColor: ['#4bd6e7', '#84a0eb', '#9db4c7'],
                hoverOffset: 4
            }]
        },
        options: {
            responsive: true,
            plugins: {
                legend: {
                    position: 'top',
                },
                tooltip: {
                    enabled: true
                }
            }
        }
    });

    // Handle the filter change
    $(".filter1").on("change", function () {
        var opt = $(this).val();

        // Update the data based on the selected filter
        if (opt === "yearly") {
            datac = yearlyData;  // Use the Razor data for the yearly option
        } else if (opt === "monthly") {
            datac = monthlyData;  // Use the monthly data for the monthly option
        }

        // Update the chart with new data
        pichart.data.datasets[0].data = datac;
        pichart.update();
    });
});


</script>


